<?xml version="1.0" encoding="UTF-8"?>
<!--
  🔒 OWASP Dependency-Check 抑制规则配置
  作用：忽略误报、已评估的低风险漏洞，减少噪音
  维护：每次更新抑制规则时，必须添加注释说明原因和评估结果
  文档：https://jeremylong.github.io/DependencyCheck/general/suppression.html
-->
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">

    <!--
    ========================================
    示例：抑制特定 CVE（已评估为不适用）
    ========================================
    <suppress>
       <notes>
           CVE-2024-xxxxx 仅影响 Windows 平台，本项目仅部署在 Linux 环境，风险不适用
           评估人：BaSui
           评估日期：2025-10-29
       </notes>
       <cve>CVE-2024-xxxxx</cve>
    </suppress>
    -->

    <!--
    ========================================
    示例：抑制特定依赖的所有漏洞（测试专用依赖）
    ========================================
    <suppress>
       <notes>
           Testcontainers 仅用于集成测试，不会打包到生产环境
           评估人：BaSui
           评估日期：2025-10-29
       </notes>
       <gav regex="true">^org\.testcontainers:.*</gav>
    </suppress>
    -->

    <!--
    ========================================
    当前抑制规则
    ========================================
    -->

    <!-- 示例：如果有需要抑制的 CVE，在此添加 -->
    <!--
    <suppress>
       <notes>
           示例：抑制某个已知的误报
           原因：该 CVE 不影响我们的使用场景
           评估人：Your Name
           评估日期：YYYY-MM-DD
       </notes>
       <cve>CVE-YYYY-XXXXX</cve>
    </suppress>
    -->

</suppressions>
